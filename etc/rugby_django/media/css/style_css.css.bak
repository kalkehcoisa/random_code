body { margin:0px; padding:0px; color:#556590; font-family:'Tahoma'; font-size:13px;}
dl { margin:0px; padding:0px;}
dl dd { margin:0px; padding:0px;}
img {border:none;}
.clear { clear:both;}

input { border:solid #556590 1px; color:#192954; font-family:'Tahoma';}
textarea { border:solid #556590 1px; color:#192954;}
select { border:solid #556590 1px; color:#192954;}

a.azul:link, 
a.azul:active, 
a.azul:visited { color:#3d4b83;}
a.azul:hover { color:#132468;}

a.bold:link, 
a.bold:active, 
a.bold:visited { font-weight:bold;}
a.bold:hover { font-weight:bold;}

a.underline:link, 
a.underline:active, 
a.underline:visited { text-decoration:underline}
a.underline:hover { text-decoration:none;}

a.menu:link,
a.menu:active,
a.menu:visited { display:block; padding:3px 6px 3px 6px; float:left; font-size:11px;}

a.letras:link, 
a.letras:active, 
a.letras:visited { color:#8bb886; margin:0px 5px 0px 5px; }
a.letras:hover { text-decoration:none;}


a.menu:hover { display:block;}

a.m_verde:link,
a.m_verde:active,
a.m_verde:visited { 
	background-color:#7da26b; 
	color:#fff; 
	text-decoration:none; 
	margin:20px 0px 0px 5px; 
	border-top:solid 2px #dff2dd;
	border-left:solid 2px #dff2dd;
	border-right:solid 2px #dff2dd;
	}
a.m_verde:hover { 
	background-color:#dff2dd; 
	color:#177e0c;
	}

